{
  "name": "skeet-api",
  "version": "0.6.3",
  "description": "Skeet API - Full-stack TypeScript Serverless Framework",
  "main": "dist/index.js",
  "repository": "https://github.com/elsoul/skeet-api.git",
  "author": "POPPIN-FUMI",
  "license": "Apache-2.0",
  "private": false,
  "scripts": {
    "test": "jest --coverage=false --detectOpenHandles --maxWorkers=1",
    "update:packages": "ncu -u && yarn",
    "build": "npx ts-node build.ts",
    "publish": "npm publish",
    "s": "npx ts-node -r tsconfig-paths/register --transpile-only src/index.ts hey",
    "dev": "nodemon",
    "start": "node dist/index.js",
    "db:deploy": "npx prisma migrate deploy",
    "db:seed": "npx prisma db seed"
  },
  "dependencies": {
    "@apollo/server": "4.4.0",
    "@google-cloud/tasks": "3.1.0",
    "@jcm/nexus-plugin-relay-global-id": "0.2.0",
    "@jcm/nexus-plugin-relay-node-interface": "0.2.0",
    "@prisma/client": "4.11.0",
    "crypto": "1.0.1",
    "dotenv": "16.0.3",
    "firebase-admin": "11.4.1",
    "graphql": "16.6.0",
    "graphql-depth-limit": "1.1.0",
    "graphql-middleware": "6.1.33",
    "graphql-query-complexity": "0.12.0",
    "graphql-relay": "0.10.0",
    "graphql-shield": "7.6.5",
    "jsonwebtoken": "9.0.0",
    "nexus": "1.3.0",
    "nexus-prisma": "1.0.4",
    "node-env-run": "4.0.2",
    "node-fetch": "2.6.7",
    "prisma": "4.11.0"
  },
  "devDependencies": {
    "@types/cors": "2.8.13",
    "@types/express": "4.17.15",
    "@types/graphql-depth-limit": "1.1.3",
    "@types/jest": "29.2.5",
    "@types/node": "18.11.18",
    "@types/node-fetch": "2.6.2",
    "@types/superagent": "4.1.16",
    "@types/supertest": "2.0.12",
    "babel-loader": "9.1.2",
    "esbuild": "0.17.7",
    "eslint": "8.31.0",
    "eslint-config-prettier": "8.6.0",
    "jest": "29.3.1",
    "nodemon": "2.0.20",
    "npm-check-updates": "16.6.2",
    "prettier": "2.8.2",
    "superagent": "8.0.6",
    "supertest": "6.3.3",
    "ts-jest": "29.0.4",
    "ts-loader": "9.4.2",
    "tsconfig-paths": "4.1.2",
    "typescript": "4.9.4"
  }
